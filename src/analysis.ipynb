"'''\n",
"\n",
"with gr.Blocks(css=css) as demo:\n",
"    gr.Markdown('# ğŸ¤– AI ë©”ë‰´ & ë§›ì§‘ ì¶”ì²œ ì‹œìŠ¤í…œ')\n",
"\n",
"    with gr.Tabs():\n",
"\n",
"        # ğŸ¯ ìš´ì„¸ íƒ­\n",
"        with gr.Tab('ğŸ¯ ì˜¤ëŠ˜ì˜ ë§›ì§‘ ìš´ì„¸'):\n",
"            spinning = gr.State(True)\n",
"            last_menu = gr.State('')\n",
"            fortune_text = gr.HTML()\n",
"            stop_btn = gr.Button('ğŸ° STOP')\n",
"            retry_btn = gr.Button('ğŸ”„ ë‹¤ì‹œ í•˜ê¸°', visible=False)\n",
"            fortune_result = gr.Markdown()\n",
"\n",
"            timer = gr.Timer(0.005, active=True)\n",
"            timer.tick(spin_menu, spinning, [fortune_text, last_menu])\n",
