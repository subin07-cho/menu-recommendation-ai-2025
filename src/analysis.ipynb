"\n",
"# =========================\n",
"# 5. ğŸ½ï¸ AI ë©”ë‰´ ì¶”ì²œ\n",
"# =========================\n",
"def recommend_menu(food, texture):\n",
"    if not food or not texture:\n",
"        return 'â— ë²„íŠ¼ì„ ì„ íƒí•˜ì„¸ìš”!'\n",
"    menus = sum(menu_map[food].values(), []) if texture == 'ì „ì²´' else menu_map[food].get(texture, [])\n",
"    menu = np.random.choice(menus)\n",
"    candidates = df[df['ë¦¬ë·°'].astype(str).str.contains(menu, na=False)]\n",
"    if candidates.empty:\n",
"        return f'### ğŸ½ï¸ ì¶”ì²œ ë©”ë‰´\\n**{menu}**\\nâŒ ë§›ì§‘ ì—†ìŒ'\n",
"    top = candidates.sort_values('í‰ì ', ascending=False).iloc[0]\n",
"    return f'## ğŸ½ï¸ {menu}\\nâ­ **{top[\\'ì‹ë‹¹ëª…\\']}**\\nğŸ“ {top[\\'ìœ„ì¹˜(ì§€í•˜ì² ì—­)\\']}'\n",
"\n",
"# =========================\n",
"# 6. UI\n",
"# =========================\n",
"css = '''\n",
".slot-running { animation: slotShake 0.08s infinite; opacity:0.2; }\n",
